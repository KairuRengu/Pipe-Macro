Sub AddPipeToCell()
    Dim CellValue As String
    For j = 13 To 100 'number of columns affected
        For i = 7 To 4000 'number of rows affected
            CellValue = ActiveSheet.Cells(i, j).Value
            If CellValue <> "" Then
                ActiveSheet.Cells(i, j).Value = "|" + CellValue
            Else
                ActiveSheet.Cells(i, j).Value = ""
            End If
        
        Next i
    Next j
End Sub
